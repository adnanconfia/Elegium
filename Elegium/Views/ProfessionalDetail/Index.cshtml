@{
    ViewData["Title"] = ViewBag.Name;
    Layout = null;
}
<style>
    .icon-container {
        width: 96px;
        height: 96px;
        position: relative;
    }
    .fade:not(.show) {
        opacity: 1;
    }

    .social-links li a {
        background:none;
        height:none ;
        border-radius: 50%;
        color: #9aa0ac;
        display: inline-block;
        width: 1.75rem;
        line-height: 1.75rem;
        text-align: center;
    }

        .icon-container::before {
            background: #42ba96;
        }

        .icon-container.avatar-offline::before,
        .icon-container.avatar-online::before {
            position: absolute;
            border: 2px solid #fff;
            display: block;
            bottom: 5px;
            right: 8px;
            content: "";
            border-radius: 50%;
            -webkit-transform: translate(-10%,0);
            transform: translate(-10%,0);
            height: 10%;
            min-height: 15%;
            width: 10%;
            min-width: 15%;
        }

        .icon-container.avatar-online::before {
            background: #009688;
        }

        .icon-container.avatar-offline::before {
            background: #d70000;
        }
</style>
<div id="printJS-form" style="max-width:1300px">
    <div class="section-body mt-2">
        <div class="container-fluid">
            <div class="row clearfix">
                <div class="col-md-12">
                    <div class="card card-profile">
                        <div class="card-body">
                            @if (ViewBag.ShowEditBtn)
                            {
                                <div class="row">
                                    <div class="col-sm-12">
                                        <div class="text-right">
                                            <div class="card-title text-right">
                                                <a ui-sref="profile" class="btn btn-outline-primary btn-sm">
                                                    <span class="fa fa-edit"></span> Edit
                                                </a>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            }
                            <div class="row">
                                <div class="col-sm-12 text-center">
                                    @if (!ViewBag.ShowEditBtn)
                                    {
                                        <button class="btn btn-primary mt-2 pull-left" style="border-radius:30px" ng-click="openHireRequest()">
                                            <span class="fa fa-send"></span> Send hire request
                                        </button>

                                        <button role="button" class="btn {{userProfile.Following?'btn-secondary':'btn-primary'}} pull-right" ng-click="toggleFollowing(userProfile)"
                                                style="border-radius:30px">
                                            {{userProfile.Following?'Following':'Follow'}}
                                        </button>
                                    }


                                    <div class="icon-container {{userProfile.Online?'avatar-online':'avatar-offline'}} m-auto d-block {{userProfile.UserId}}">
                                        <img class="card-profile-img" ng-src="/api/UserProfiles/GetUserPhoto/{{userProfile.UserId}}/100/100" alt="">
                                        <span class="status-circle"></span>
                                    </div>
                                    <h4>
                                        {{userProfile.FirstName}} {{userProfile.LastName}}
                                    </h4>
                                    <h6>
                                        {{userProfile.CompanyPosition.Name}}
                                    </h6>
                                    <p>
                                        <i ng-if="userProfile.Country || userProfile.City" class="fa fa-map-marker text-gray"></i>
                                        <span>{{userProfile.Country.Name}}</span>
                                        <span ng-if="userProfile.Country && userProfile.City">,</span>
                                        <span>{{userProfile.City.Name}}</span>
                                    </p>
                                    <ul class="social-links list-inline mb-3 mt-2">
                                        <li class="list-inline-item" ng-if="userProfile.FacebookLinkIsPublic">
                                            <a href="{{userProfile.FacebookLink}}" target="_blank" title="Facebook" data-toggle="tooltip">
                                                <img src="data:image/svg+xml;base64,"
                                                     height="40" />
                                            </a>
                                        </li>
                                        <li class="list-inline-item" ng-if="userProfile.TwitterLinkIsPublic">
                                            <a href="{{userProfile.TwitterLink}}" target="_blank" title="Twitter" data-toggle="tooltip">
                                                <img height="40" 
                                                     src="data:image/svg+xml;base64," />
                                            </a>
                                        </li>
                                        <li class="list-inline-item" ng-if="userProfile.ImdbLinkIsPublic">
                                            <a href="{{userProfile.ImdbLink}}" target="_blank" title="IMDB" data-toggle="tooltip">
                                                <img 
                                                     height="40"
                                                     src="data:image/svg+xml;base64," />
                                            </a>
                                        </li>
                                    </ul>
                                    <button class="btn btn-outline-primary btn-sm" tooltip-placement="top" uib-tooltip="Contact this professional" ng-click="contactUser()"><span class="material-icons">email</span></button>
                                    <button class="btn btn-outline-primary btn-sm ml-2" ng-if="false" ng-click="print()" ><span class="material-icons">forward</span></button>
                                    <button class="btn btn-outline-primary btn-sm ml-2" ng-click="forwardUser()" tooltip-placement="top" uib-tooltip="Forward this profile to your friend"><span class="material-icons">forward</span></button>
                                    @if (!ViewBag.ShowEditBtn)
                                    {
                                        <button class="btn btn-outline-primary btn-sm ml-2" ng-click="openChatBox(userProfile.UserId)" tooltip-placement="top" uib-tooltip="Send a message"><span class="material-icons">message</span></button>
                                    }
                                    <a tooltip-placement="top" uib-tooltip="Share this profile on Social Media" class="btn btn-outline-primary btn-sm ml-2" data-toggle="collapse" data-target="#social" aria-expanded="false" aria-controls="collapseExample">
                                        <span class="material-icons">share</span>
                                    </a>
                                    <div id="social" class="collapse">
                                        <div class="">
                                            <ul class="sticky">
                                                <li class="twitter">
                                                    <a href="https://twitter.com/intent/tweet?url={{url}}" target="_blank">
                                                        <img height="40"
                                                             src="data:image/svg+xml;base64," />
                                                    </a>
                                                </li>
                                                <li class="facebook">
                                                    <a href="https://www.facebook.com/sharer/sharer.php?u={{url}}" target="_blank">
                                                        <img src="data:image/svg+xml;base64,"
                                                             height="40" />
                                                    </a>
                                                </li>
                                                <li class="pinterest">
                                                    <a href="https://pinterest.com/pin/create/button/?url={{url}}" target="_blank">
                                                        <img 
                                                             height="40"
                                                             src="data:image/svg+xml;base64," />
                                                    </a>
                                                </li>
                                                <li class="linkedin">
                                                    <a href="https://www.linkedin.com/shareArticle?mini=true&url={{url}}" target="_blank">
                                                        <img 
                                                             height="40"
                                                             src="data:image/svg+xml;base64," />
                                                    </a>
                                                </li>
                                            </ul>
                                        </div>
                                    </div>

                                    <hr />
                                    <label class="col-form-label">{{userProfile.IntroText}}</label>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
