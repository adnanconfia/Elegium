                                    <div class="col-sm-6 col-md-4">
                                        <div class="form-group">
                                            <label class="form-label">Select Overlay Opacity</label>
                                            <select ng-model="userProfile.BgOpacity" class="form-control dropdown" ng-options="x for x in opacity">
                                            </select>
                                        </div>
                                    </div>
                                    <div class="col-sm-6 col-md-4 p-4">
                                        <input type="file" id="bgSelector" hidden accept="image/*" onchange="angular.element(this).scope().uploadBgImage(event)" />
                                        <button type="button" ng-click="openBgFileSelector()" class="btn btn-success">Select Background Image</button>
                                    </div>
                                </div>
                                <div class="row clearfix">
                                    <div class="col-md-12" style="background-image: url('api/UserProfiles/GetBGImage/U/1');
                                         background-repeat: no-repeat;
                                         background-size: cover;
                                         background-attachment: fixed;
                                         background-position: center top;
                                         background-blend-mode: overlay;
                                         height:100vh;
                                         box-shadow: inset 0 0 0 2000px {{hexToRgbA(userProfile.BgColor, userProfile.BgOpacity)}};">
                                        @*<img id="bgImage" class="img-fluid" src="/api/UserProfiles/GetBGImage/U/0" />*@
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>



