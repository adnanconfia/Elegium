
@{ ViewData["Title"] = "Profile";
                Layout = null; }

<link href="~/css/step-wizard.css" rel="stylesheet" />

<div class="section-body" ng-controller="ProfileController" style="max-width:1300px">
    <div class="container-fluid">
        <div class="d-flex justify-content-between align-items-center">
            <ul class="nav nav-tabs page-header-tab">
                <li class="nav-item"><a class="nav-link active" id="general-tab" data-toggle="tab" data-target="#general-info" ng-click="setClass()">General Info</a></li>
                <li class="nav-item"><a class="nav-link" id="company-tab" data-toggle="tab" data-target="#company-info" ng-click="setClass()">Company Info</a></li>
                <li class="nav-item"><a class="nav-link" id="portfolio-tab" data-toggle="tab" data-target="#portfolio-info" ng-click="setClass()">Portfolio</a></li>
                <li class="nav-item"><a class="nav-link" id="selfPromotion-tab" data-toggle="tab" data-target="#self-promotion" ng-click="setClass()">Self Promotion</a></li>
                @*<li class="nav-item"><a class="nav-link" id="equipments-tab" data-toggle="tab" href="#equipments">Equipments</a></li>*@
                <li class="nav-item"><a class="nav-link" id="weblinks-tab" data-toggle="tab" data-target="#web-links" ng-click="setClass()">Web & Links</a></li>
                <li class="nav-item"><a class="nav-link" id="public-preview-tab" data-toggle="tab" data-target="#public-preview" ng-click="setClass('PP')">Public Preview</a></li>
                <li class="nav-item" ng-if="false"><a class="nav-link" id="bg-image-tab" data-toggle="tab" data-target="#bg-image" ng-click="setClass('PP')">Background Image</a></li>
            </ul>
            <div class="header-action d-md-flex">
                <button type="button" class="btn btn-primary" ng-click="saveUserProfile()"><i class="fe fe-save mr-2"></i>Save</button>
            </div>
        </div>

        <div class="row clearfix">
            <div class="col-12">
                <div class="card">
                    <div class="card-body">
                        <div class="tab-content">
                            <div class="tab-pane fade show active" id="general-info">
                                <div class="row clearfix">
                                    <div class="col-sm-12">
                                        <div class="row">
                                            <div class="col-sm-6">
                                                <button class="btn btn-success" ng-click="openCroppie()">Upload profile Photo</button>
                                            </div>
                                            <div class="col-sm-6 text-right">
                                                <img style="border-radius:100%" ng-src="{{UserPhoto}}" width="100" />
                                            </div>
                                        </div>
                                        <hr />
                                        <div class="row">
                                            <div class="col-sm-6 col-md-4">
                                                <div class="form-group">
                                                    <label class="form-label">First Name</label>
                                                    <input type="text" class="form-control" ng-model="userProfile.FirstName" placeholder="First Name">
                                                </div>
                                            </div>
                                            <div class="col-sm-6 col-md-4">
                                                <div class="form-group">
                                                    <label class="form-label">Middle Name</label>
                                                    <input type="text" class="form-control" ng-model="userProfile.MiddleName" placeholder="Middle Name">
                                                </div>
                                            </div>
                                            <div class="col-sm-6 col-md-4">
                                                <div class="form-group">
                                                    <label class="form-label">Last Name</label>
                                                    <input type="text" class="form-control" ng-model="userProfile.LastName" placeholder="Last Name">
                                                </div>
                                            </div>
                                            <div class="col-sm-6 col-md-6">
                                                <div class="form-group">
                                                    <label class="form-label">Street Address</label>
                                                    <input type="text" class="form-control" ng-model="userProfile.StreetAddress" placeholder="Street Address">
                                                </div>
                                            </div>
                                            <div class="col-sm-6 col-md-6">
                                                <div class="form-group">
                                                    <label class="form-label">Street Address 2</label>
                                                    <input type="text" class="form-control" ng-model="userProfile.StreetAddress2" placeholder="Flat, suite, unit, building, floor, etc." value="">
                                                </div>
                                            </div>
                                            <div class="col-sm-6 col-md-4">
                                                <div class="form-group">
                                                    <label class="form-label">Country/Region</label>
                                                    <ui-select class="form-control" ng-model="userProfile.CountryId" theme="selectize" sortable="true" close-on-select="true">
                                                        <ui-select-match placeholder="Select Country...">{{$select.selected.Name}}</ui-select-match>
                                                        <ui-select-choices repeat="item.Id as item in countries | filter: $select.search">
                                                            <div ng-bind-html="item.Name | highlight: $select.search"></div>

                                                        </ui-select-choices>
                                                    </ui-select>
                                                    <!--<select class="form-control custom-select" ng-options="x.Id as x.Name for x in countries"
                                                            ng-model="userProfile.CountryId">-->
                                                        @*ng-change="getCitiesOfCountry(userProfile.CountryId, 'personal')">*@
                                                    <!--</select>-->
                                                </div>
                                            </div>
                                            <div class="col-sm-6 col-md-4">
                                                <div class="form-group">
                                                    <label class="form-label">City</label>
                                                    <input type="text" class="form-control" ng-model="userProfile.CityName" placeholder="City" value="">
                                                    @*<select class="form-control custom-select" ng-options="x.Id as x.Name for x in cities" ng-model="userProfile.CityId">
                                                    </select>*@
                                                </div>
                                            </div>
                                            <div class="col-sm-6 col-md-4">
                                                <div class="form-group">
                                                    <label class="form-label">State/Province/Region</label>
                                                    <input type="text" class="form-control" ng-model="userProfile.State" placeholder="State/Province/Region" value="">
                                                </div>
                                            </div>
                                            <div class="col-sm-6 col-md-4">
                                                <div class="form-group">
                                                    <label class="form-label">Postal Code</label>
                                                    <input type="text" class="form-control" ng-model="userProfile.PostCode" placeholder="ZIP Code">
                                                </div>
                                            </div>
                                            <div class="col-sm-6 col-md-4">
                                                <div class="form-group">
                                                    <label class="form-label">Age</label>
                                                    <input type="text" class="form-control" ng-model="userProfile.Age" placeholder="Age">
                                                </div>
                                            </div>
                                            <div class="col-sm-6 col-md-4">
                                                <div class="form-group">
                                                    <label class="form-label">Height</label>
                                                    <input type="text" class="form-control" ng-model="userProfile.Height" placeholder="Height">
                                                </div>
                                            </div>
                                            <div class="col-sm-6 col-md-4">
                                                <div class="form-group">
                                                    <label class="form-label">Weight</label>
                                                    <input type="text" class="form-control" ng-model="userProfile.Weight" placeholder="Weight">
                                                </div>
                                            </div>
                                            <div class="col-sm-6 col-md-4">
                                                <div class="form-group">
                                                    <label class="form-label">Eye Color</label>
                                                    <input type="text" class="form-control" ng-model="userProfile.EyeColor" placeholder="Eye Color">
                                                </div>
                                            </div>
                                            <div class="col-sm-6 col-md-4">
                                                <div class="form-group">
                                                    <label class="form-label">Gender</label>
                                                    <input type="text" class="form-control" ng-model="userProfile.HairColor" placeholder="Hair Color">
                                                </div>
                                            </div>
                                            <div class="col-sm-6 col-md-4">
                                                <div class="form-group">
                                                    <label class="form-label">Hair Color</label>
                                                    <input type="text" class="form-control" ng-model="userProfile.HairColor" placeholder="Hair Color">
                                                </div>
                                            </div>
                                            <div class="col-sm-6 col-md-4">
                                                <div class="form-group">
                                                    <label class="form-label">Build</label>
                                                    <input type="text" class="form-control" ng-model="userProfile.Build" placeholder="Build">
                                                </div>
                                            </div>
                                            <div class="col-sm-6 col-md-4">
                                                <div class="form-group">
                                                    <label class="form-label">Skin Color</label>
                                                    <input type="text" class="form-control" ng-model="userProfile.SkinColor" placeholder="Skin Color">
                                                </div>
                                            </div>
                                            <div class="col-md-12">
                                                <div class="form-group">
                                                    <label class="form-label">Intro Summary / Professional Overview</label>
                                                    <textarea rows="6" class="form-control" ng-model="userProfile.IntroText" placeholder="Keep it short and snappy - a couple of lines is perfect" value=""></textarea>
                                                </div>
                                            </div>
                                            <div class="col-sm-12 col-md-6">
                                                <div class="form-group">
                                                    <label class="form-label">Your email</label>
                                                    <input type="email" class="form-control" ng-model="userProfile.Email" placeholder="Your email">
                                                </div>
                                            </div>
                                            <div class="col-sm-12 col-md-6">
                                                <div class="form-group">
                                                    <label class="form-label">Skype</label>
                                                    <input type="email" class="form-control" ng-model="userProfile.Skype" placeholder="Skype">
                                                </div>
                                            </div>
                                            <div class="col-sm-12 col-md-6">
                                                <div class="form-group">
                                                    <label class="form-label">What is your English Proficiency?</label>
                                                    <ui-select class="form-control" ng-model="userProfile.EnglishLevelId" theme="selectize" sortable="true" close-on-select="true">
                                                        <ui-select-match placeholder="Select English level...">{{$select.selected.Name}}</ui-select-match>
                                                        <ui-select-choices repeat="item.Id as item in languageLevels | filter: $select.search">
                                                            <div ng-bind-html="item.Name | highlight: $select.search"></div>

                                                        </ui-select-choices>
                                                    </ui-select>
                                                    @*<select class="form-control custom-select" ng-options="item.Id as item.Name for item in languageLevels" ng-model="userProfile.EnglishLevelId">
                                                    </select>*@
                                                </div>
                                            </div>
                                            <div class="col-sm-12 col-md-6">
                                                <div class="form-group">
                                                    <label class="form-label">What other lanuages do you speak?</label>
                                                    <ui-select multiple class="form-control" ng-model="userOtherLanguages.langs" theme="selectize" sortable="true" close-on-select="false">
                                                        <ui-select-match placeholder="Select Languages...">{{$item.Name}}</ui-select-match>
                                                        <ui-select-choices repeat="lang.Id as lang in languages">
                                                            <div ng-bind-html="lang.Name | highlight: $select.search"></div>
                                                            @*{{lang.Name}}*@
                                                        </ui-select-choices>
                                                    </ui-select>
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                            <div class="tab-pane fade" id="company-info" role="tabpanel">
                                <div class="row clearfix">
                                    <div class="col-sm-12">
                                        <div class="row">
                                            <div class="col-sm-6">
                                                <button class="btn btn-success" ng-click="openCroppieCompany()">Upload Company Logo</button>
                                            </div>
                                            <div class="col-sm-6 text-right">
                                                <img style="border-radius:100%" ng-src="{{CompanyPhoto}}" width="100" />
                                            </div>
                                        </div>
                                        <hr />
                                        <div class="row">
                                            <div class="col-sm-6 col-md-6">
                                                <div class="form-group">
                                                    <label class="form-label">Company Name</label>
                                                    <input type="text" class="form-control" ng-model="userProfile.CompanyName" placeholder="Company Name">
                                                </div>
                                            </div>
                                            <div class="col-sm-6 col-md-6">
                                                <div class="form-group">
                                                    <label class="form-label">Company Type</label>
                                                    <ui-select class="form-control" ng-model="userProfile.CompanyTypeId" theme="selectize" sortable="true" close-on-select="true">
                                                        <ui-select-match placeholder="Select working position...">{{$select.selected.Name}}</ui-select-match>
                                                        <ui-select-choices repeat="item.Id as item in companyTypes | filter: $select.search">
                                                            <div ng-bind-html="item.Name | highlight: $select.search"></div>

                                                        </ui-select-choices>
                                                    </ui-select>
                                                </div>
                                            </div>
                                            <div class="col-sm-6 col-md-6">
                                                <div class="form-group">
                                                    <label class="form-label">What is your working position?</label>
                                                    <ui-select class="form-control" ng-model="userProfile.CompanyPositionId" theme="selectize" sortable="true" close-on-select="true">
                                                        <ui-select-match placeholder="Select working position...">{{$select.selected.Name}}</ui-select-match>
                                                        <ui-select-choices repeat="item.Id as item in workingPositions | filter: $select.search">
                                                            <div ng-bind-html="item.Name | highlight: $select.search"></div>

                                                        </ui-select-choices>
                                                    </ui-select>
                                                </div>
                                            </div>
                                            <div class="col-sm-6 col-md-6">
                                                <div class="form-group">
                                                    <div class="form-label">Are you a legal representant of the company?</div>
                                                    <label class="custom-switch">
                                                        <input type="checkbox" ng-model="userProfile.CompanyLegalRepresent" name="custom-switch-checkbox" class="custom-switch-input">
                                                        <span class="custom-switch-indicator"></span>
                                                    </label>
                                                </div>
                                            </div>
                                        </div>
                                        <div class="row">
                                            <div class="col-sm-6 col-md-6">
                                                <div class="form-group">
                                                    <label class="form-label">Company Office Street Address</label>
                                                    <input type="text" class="form-control" ng-model="userProfile.CompanyStreetAddress" placeholder="Street Address">
                                                </div>
                                            </div>
                                            <div class="col-sm-6 col-md-6">
                                                <div class="form-group">
                                                    <label class="form-label">Street Address 2</label>
                                                    <input type="text" class="form-control" ng-model="userProfile.CompanyStreetAddress2" placeholder="Flat, suite, unit, building, floor, etc." value="">
                                                </div>
                                            </div>
                                            <div class="col-sm-6 col-md-4">
                                                <div class="form-group">
                                                    <label class="form-label">Country/Region</label>
                                                    <ui-select class="form-control" ng-model="userProfile.CompanyCountryId" theme="selectize" sortable="true" close-on-select="true">
                                                        <ui-select-match placeholder="Select Country...">{{$select.selected.Name}}</ui-select-match>
                                                        <ui-select-choices repeat="item.Id as item in countries | filter: $select.search">
                                                            <div ng-bind-html="item.Name | highlight: $select.search"></div>

                                                        </ui-select-choices>
                                                    </ui-select>
                                                    <!--<select class="form-control custom-select" ng-options="x.Id as x.Name for x in countries"
                                                            ng-model="userProfile.CompanyCountryId">-->
                                                        @*ng-change="getCitiesOfCountry(userProfile.CompanyCountryId, 'company')">*@
                                                    <!--</select>-->
                                                </div>
                                            </div>
                                            <div class="col-sm-6 col-md-4">
                                                <div class="form-group">
                                                    <label class="form-label">City</label>
                                                    <input type="text" class="form-control" ng-model="userProfile.CompanyCityName" placeholder="City" value="">
                                                    @*<select class="form-control custom-select" ng-options="x.Id as x.Name for x in companyCities" ng-model="userProfile.CompanyCityId">
                                                        </select>*@
                                                </div>
                                            </div>
                                            <div class="col-sm-6 col-md-4">
                                                <div class="form-group">
                                                    <label class="form-label">State/Province/Region</label>
                                                    <input type="text" class="form-control" ng-model="userProfile.CompanyState" placeholder="State/Province/Region" value="">
                                                </div>
                                            </div>
                                            <div class="col-sm-6 col-md-4">
                                                <div class="form-group">
                                                    <label class="form-label">Postal Code</label>
                                                    <input type="text" class="form-control" ng-model="userProfile.CompanyPostCode" placeholder="ZIP Code">
                                                </div>
                                            </div>
                                        </div>
                                        <div class="row">
                                            <div class="col-sm-12 col-md-6">
                                                <div class="form-group">
                                                    <label class="form-label">Company's official email</label>
                                                    <input type="email" class="form-control" ng-model="userProfile.CompanyEmail" placeholder="Studio's official email">
                                                </div>
                                            </div>
                                            <div class="col-sm-12 col-md-6">
                                                <div class="form-group">
                                                    <label class="form-label">Company's official web</label>
                                                    <input type="text" class="form-control" ng-model="userProfile.CompanyWeb" placeholder="Studio's official web">
                                                </div>
                                            </div>
                                            <div class="col-sm-12 col-md-6">
                                                <div class="form-group">
                                                    <label class="form-label">Company number</label>
                                                    <input type="text" class="form-control" ng-model="userProfile.CompanyPhone" placeholder="Company number">
                                                </div>
                                            </div>
                                        </div>
                                        <br />
                                        <b><u>Studio or Production Premises address if different:</u></b>
                                        <br />
                                        <br />
                                        <div class="row">
                                            <div class="col-sm-6 col-md-6">
                                                <div class="form-group">
                                                    <label class="form-label">Company Studio Street Address</label>
                                                    <input type="text" class="form-control" ng-model="userProfile.CompanyStudioAddress" placeholder="Street Address">
                                                </div>
                                            </div>
                                            <div class="col-sm-6 col-md-6">
                                                <div class="form-group">
                                                    <label class="form-label">Street Address 2</label>
                                                    <input type="text" class="form-control" ng-model="userProfile.CompanyStudioAddress2" placeholder="Flat, suite, unit, building, floor, etc.">
                                                </div>
                                            </div>
                                            <div class="col-sm-6 col-md-4">
                                                <div class="form-group">
                                                    <label class="form-label">Country/Region</label>
                                                    <ui-select class="form-control" ng-model="userProfile.CompanyStudioCountryId" theme="selectize" sortable="true" close-on-select="true">
                                                        <ui-select-match placeholder="Select Country...">{{$select.selected.Name}}</ui-select-match>
                                                        <ui-select-choices repeat="item.Id as item in countries | filter: $select.search">
                                                            <div ng-bind-html="item.Name | highlight: $select.search"></div>

                                                        </ui-select-choices>
                                                    </ui-select>
                                                    <!--<select class="form-control custom-select" ng-options="x.Id as x.Name for x in countries"
                                                            ng-model="userProfile.CompanyStudioCountryId">-->
                                                        @*ng-change="getCitiesOfCountry(userProfile.CompanyStudioCountryId, 'studio')">*@
                                                    <!--</select>-->
                                                </div>
                                            </div>
                                            <div class="col-sm-6 col-md-4">
                                                <div class="form-group">
                                                    <label class="form-label">City</label>
                                                    <input type="text" class="form-control" ng-model="userProfile.CompanyStudioCityName" placeholder="City" value="">

                                                    @*<select class="form-control custom-select" ng-options="x.Id as x.Name for x in studioCities" ng-model="userProfile.CompanyStudioCityId">
                                                        </select>*@
                                                </div>
                                            </div>
                                            <div class="col-sm-6 col-md-4">
                                                <div class="form-group">
                                                    <label class="form-label">State/Province/Region</label>
                                                    <input type="text" class="form-control" ng-model="userProfile.CompanyStudioState" placeholder="State/Province/Region">
                                                </div>
                                            </div>
                                            <div class="col-sm-6 col-md-4">
                                                <div class="form-group">
                                                    <label class="form-label">Postal Code</label>
                                                    <input type="text" class="form-control" ng-model="userProfile.CompanyStudioPostCode" placeholder="ZIP Code">
                                                </div>
                                            </div>
                                        </div>
                                        <div class="row">
                                            <div class="col-sm-12 col-md-6">
                                                <div class="form-group">
                                                    <label class="form-label">Studio's official email</label>
                                                    <input type="email" class="form-control" ng-model="userProfile.CompanyStudioEmail" placeholder="Studio's official email">
                                                </div>
                                            </div>
                                            <div class="col-sm-12 col-md-6">
                                                <div class="form-group">
                                                    <label class="form-label">Studio's official web</label>
                                                    <input type="text" class="form-control" ng-model="userProfile.CompanyStudioWeb" placeholder="Studio's official web">
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                            <div class="tab-pane fade" id="portfolio-info" role="tabpanel">
                                <div class="d-flex justify-content-between align-items-center">
                                    <ul class="nav nav-tabs page-header-tab">
                                        <li class="nav-item"><a class="nav-link active" id="photos-tab" data-toggle="tab" data-target="#photos">Add Photos</a></li>
                                        <li class="nav-item"><a class="nav-link" id="videos-tab" data-toggle="tab" data-target="#videos">Add Videos</a></li>
                                        <li class="nav-item"><a class="nav-link" id="audios-tab" data-toggle="tab" data-target="#audios">Add Audios</a></li>
                                    </ul>
                                </div>
                                <div class="row clearfix">
                                    <div class="col-sm-12">
                                        <div class="card bg-trans">
                                            <div class="tab-content">
                                                <div id="photos" class="tab-pane fade active show">
                                                    <div class="row clearfix">
                                                        <div class="col-sm-12">
                                                            <div class="row mb-2">
                                                                <div class="col-sm-12">
                                                                    <label class="col-form-label">
                                                                        <i class="fa fa-play m-sm-1"></i>
                                                                        Add only a photos showing and representing your own work and experience.
                                                                    </label><br />
                                                                    Photos can get you hired.
                                                                </div>
                                                            </div>
                                                            <div class="row bg-trans" style="background: #f1f1f1;">
                                                                <div class="col-6 col-md-4 col-xl-2 m-sm-2">
                                                                    <div class="card">
                                                                        <div class="card-title text-center border-bottom">
                                                                            <span class="small">{{totalUserPhotos}} Photos</span>
                                                                        </div>
                                                                        <div class="card-body">
                                                                            <a href="javascript:void(0)" ng-click="openFileSelector('P')" class="my_sort_cut text-muted">
                                                                                <i class="icon-plus"></i>
                                                                                <span>Add Photos</span>
                                                                            </a>
                                                                        </div>
                                                                    </div>
                                                                </div>
                                                                <input type="file" accept=".jpg,.jpeg,.png,.gif,.bmp" name="fileUpload" id="fileUpload" multiple hidden />
                                                                <div class="col-6 col-md-4 col-xl-2 m-sm-2">
                                                                    <div class="card">
                                                                        <div class="card-title text-center border-bottom">
                                                                            <span class="small">{{totalUserAlbums}} Albums</span>
                                                                        </div>
                                                                        <div class="card-body">
                                                                            <a href="javascript:void(0)" ng-click="openFileSelector('A')" class="my_sort_cut text-muted">
                                                                                <i class="icon-plus"></i>
                                                                                <span>Create album</span>
                                                                            </a>
                                                                        </div>
                                                                    </div>
                                                                </div>
                                                            </div>
                                                            <hr />
                                                            <div class="row bg-trans" style="background: #f1f1f1;">
                                                                <div class="col-sm-12">
                                                                    <label class="col-form-label">Albums</label>
                                                                    <div class="row row-cards">
                                                                        <div class="col-sm-6 col-lg-4" callback-fn="refreshAlbums(arg1)" obj="p" drop-on-me ng-repeat="p in userAlbums">
                                                                            <div class="card p-3">
                                                                                <a href="javascript:void(0)" id="navigateToAlbumResult" ng-click="getAlbumPhotos(p)" class="mb-3 text-center">
                                                                                    @*<img class="rounded" aria-label="{{p.Name}}" alt="{{p.Name}}" ng-src="api/UserProfiles/GetFirstFileInAlbum/{{p.Id}}" />*@
                                                                                </a>
                                                                                <div class="text-center">
                                                                                    <ul class="list-unstyled team-info sm margin-0">
                                                                                        <li ng-repeat="t in p.FileIds">
                                                                                            <img style="width:100% !important" class="border-radius-10" ng-src="api/UserProfiles/GetFileThumbnail/{{t}}/70/50" alt="Avatar">
                                                                                        </li>
